{
  "env": {
    "node": true,
    "jquery": true
  },
  "rules": {
    "global-require": "off",
    "key-spacing": [
      "error",
      {
        "afterColon": true
      }
    ],
    "no-var": "off",
    "prefer-const": "off"
  },
  "globals": {
    "define": false,
    "Resources": false,
    "SitePreferences": false,
    "Urls": false,
    "WirecardPaymentPage": false
  }
}
