<?xml version="1.0" encoding="UTF-8"?>
<services xmlns="http://www.demandware.com/xml/impex/services/2014-09-26">

    <service-credential service-credential-id="wirecard.elastic.cred">
        <url>https://api-test.wirecard.com/engine/rest/paymentmethods</url>
        <user-id></user-id>
        <password encrypted="true" encryption-type="common.export"></password>
    </service-credential>

    <service-profile service-profile-id="wirecard.elastic.profile">
        <timeout-millis>5000</timeout-millis>
        <rate-limit-enabled>true</rate-limit-enabled>
        <rate-limit-calls>10</rate-limit-calls>
        <rate-limit-millis>10000</rate-limit-millis>
        <cb-enabled>true</cb-enabled>
        <cb-calls>10</cb-calls>
        <cb-millis>5000</cb-millis>
    </service-profile>

    <service service-id="wirecard.elastic">
        <service-type>HTTP</service-type>
        <enabled>true</enabled>
        <log-prefix>wirecard</log-prefix>
        <comm-log-enabled>true</comm-log-enabled>
        <mock-mode-enabled>false</mock-mode-enabled>
        <profile-id>wirecard.elastic.profile</profile-id>
        <credential-id>wirecard.elastic.cred</credential-id>
    </service>

</services>
