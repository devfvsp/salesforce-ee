<?xml version="1.0" encoding="UTF-8"?>
<metadata xmlns="http://www.demandware.com/xml/impex/metadata/2006-10-31">
    <type-extension type-id="SitePreferences">
        <custom-attribute-definitions>
            <attribute-definition attribute-id="paymentGatewayDebugMode">
                <display-name xml:lang="x-default">Debug mode</display-name>
                <description xml:lang="x-default">Write request xml to debug log (works only with sandboxes / development)</description>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <default-value>true</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="paymentGatewayShopName">
                <display-name xml:lang="x-default">Shop Name</display-name>
                <description xml:lang="x-default">Max length: 9 characters. All characters exceeding this limit will be ignored!</description>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
                <default-value>Web Shop</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="paymentGatewayPayPalAddDescriptorToRequest">
                <display-name xml:lang="x-default">Send descriptor</display-name>
                <description xml:lang="x-default">If "Yes" the parameter descriptor is included in request data sent to wpg.</description>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <default-value>true</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="paymentGatewayUrlSalt">
                <display-name xml:lang="x-default">Url salt</display-name>
                <description xml:lang="x-default">Salt for calculating url fingerprint sha512 hash</description>
                <type>string</type>
                <mandatory-flag>true</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>5</min-length>
                <default-value>-oc.nMIXT+JnR9Ka</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="paymentGatewayIdealMerchantAccountID">
                <display-name xml:lang="x-default">iDEAL: Merchant-Account-ID</display-name>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
                <default-value>4aeccf39-0d47-47f6-a399-c05c1f2fc819</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="paymentGatewaySofortMerchantAccountID">
                <display-name xml:lang="x-default">Merchant-Account-ID</display-name>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
                <default-value>6c0e7efd-ee58-40f7-9bbd-5e7337a052cd</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="paymentGatewaySofortBaseUrl">
                <display-name xml:lang="x-default">Api base-url</display-name>
