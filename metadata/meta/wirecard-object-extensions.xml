        <group-definitions>
            <attribute-group group-id="PAYMENTGATEWAY">
                <display-name xml:lang="x-default">Payment gateway settings</display-name>
                <attribute attribute-id="paymentGatewayDebugMode"/>
                <attribute attribute-id="paymentGatewayShopName"/>
                <attribute attribute-id="paymentGatewayUrlSalt"/>
                <attribute attribute-id="paymentGatewayIdealMerchantAccountID"/>
            </attribute-group>
            <attribute-group group-id="PAYMENTGATEWAY_PAYPAL">
                <display-name xml:lang="x-default">Payment gateway settings for PayPal</display-name>
                <attribute attribute-id="paymentGatewayPayPalMerchantAccountID"/>
                <attribute attribute-id="paymentGatewayPayPalBaseUrl"/>
                <attribute attribute-id="paymentGatewayPayPalHttpUser"/>
                <attribute attribute-id="paymentGatewayPayPalHttpPassword"/>
