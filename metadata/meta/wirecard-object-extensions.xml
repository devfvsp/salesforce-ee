            </attribute-definition>
        </custom-attribute-definitions>
        <group-definitions>
            <attribute-group group-id="PaymentGateway">
                <display-name xml:lang="x-default">Payment Gateway</display-name>
                <attribute attribute-id="paymentGatewayTransactions"/>
