                <display-name xml:lang="x-default">Send basket request data</display-name>
                <description xml:lang="x-default">If "yes" basket information is added to request</description>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <default-value>true</default-value>
            </attribute-definition>
