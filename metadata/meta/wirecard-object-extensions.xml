                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
                <default-value>https://api-test.wirecard.com</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="paymentGatewayPayPalHttpUser">
                <display-name xml:lang="x-default">Http user</display-name>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
                <default-value>16390-testing</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="paymentGatewayPayPalHttpPassword">
                <display-name xml:lang="x-default">Http password</display-name>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
                <default-value>3!3013=D3fD8X7</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="paymentGatewayPayPalInitialTransactionType">
                <display-name xml:lang="x-default">Transaction type for place order</display-name>
                <description xml:lang="x-default">Transaction type of request which is triggered when placing an order in the store frontend</description>
                <type>enum-of-string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <value-definitions>
                    <value-definition>
                        <display xml:lang="x-default">authorization</display>
                        <value>authorization</value>
                    </value-definition>
                    <value-definition default="true">
                        <display xml:lang="x-default">debit</display>
                        <value>debit</value>
                    </value-definition>
                </value-definitions>
            </attribute-definition>
            <attribute-definition attribute-id="paymentGatewayPayPalSendAdditionalData">
                <display-name xml:lang="x-default">Send additional request data</display-name>
                <description xml:lang="x-default">If "yes" additional data (fps) is added to request</description>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <default-value>true</default-value>
            </attribute-definition>
