                <description xml:lang="x-default">If "yes" additional data (fps) is added to request</description>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <default-value>true</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="paymentGatewaySofortSendBasketData">
                <display-name xml:lang="x-default">Send basket request data</display-name>
                <description xml:lang="x-default">If "yes" basket information is added to request</description>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <default-value>true</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="paymentGatewayPayPalMerchantAccountID">
                <display-name xml:lang="x-default">Merchant-Account-ID</display-name>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
                <default-value>8695f3e4-76e8-414b-96cd-a1e5dafcf20c</default-value>
